{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "C/C++: g++ build and debug active file",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/build/aarch64/bin/app/app_flowengine",
      // "program": "${workspaceFolder}/build/aarch64/bin/rockchip/mpp_decode",
      "args": [
               "--config_path=${workspaceFolder}/conf/app/agent.json",
               "--num_workers=32",
              
              // "--uri=rtsp://admin:zkfd123.com@114.242.23.39:9303/Streaming/Channels/101",
              ],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "miDebuggerPath": "/usr/bin/gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ],
      "preLaunchTask": "C/C++: g++ build active file"
    }
  ]
}